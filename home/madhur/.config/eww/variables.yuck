
(defpoll calendar_day :interval "10h" :initial "1" "date '+%d'")
(defpoll calendar_month :interval "10h" :initial "1" "./scripts/getCalendarMonth")
(defpoll calendar_year :interval "10h" :initial "2025" "date '+%Y'")

(defpoll lithour :interval "30m" "date +%H")
(defpoll litmin :interval "30s" "date +%M")
(defpoll completeday :interval "1h" "date '+%A, %d %B'")